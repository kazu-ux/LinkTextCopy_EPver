{
    "name": "リンクテキストコピー EPver",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Sample Chrome Extension",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "clickEvent.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "contextMenus.js"
        ],
        "persistent": false
    },
    "permissions": [
        "contextMenus",
        "activeTab"
    ]
}